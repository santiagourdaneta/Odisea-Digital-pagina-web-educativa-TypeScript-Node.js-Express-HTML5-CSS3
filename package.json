{
  "name": "proyecto-super-veloz",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --emptyOutDir",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.{ts,html,css}\"",
    "kill": "npx kill-port 3000",
    "deploy": "npm run format && npm run lint && git add . && git commit -m 'Mejora automatica' && git push origin main"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "terser": "^5.44.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.50.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express-rate-limit": "^8.2.1",
    "fs": "^0.0.1-security"
  }
}
